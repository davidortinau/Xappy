<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xappy.Content.Blog.BlogDetailPage"
              xmlns:blog="clr-namespace:Xappy.Content.Blog"
             xmlns:customviews="clr-namespace:Xappy.CustomViews"
             Title="{Binding BlogItem.Title}">
    <ContentPage.Content>
        <customviews:CustomWebView  Source="{Binding Source}"/>
    </ContentPage.Content>
</ContentPage>